<div class="nutrient-tracker">
  <div class="nutrient-tracker-header">
    <h3>Nutrient Tracker</h3>
    <div class="nutrient-tracker-view">
      <a href="#" class="selected">Daily</a><span> | </span><a href="#">Weekly</a>
    </div>
  </div>
  <div class="nutrient-tracker-body">
    <div class="nutrient-tracker-full">
      <%= render '/shared/nutrient_tracker/full_tracker' %>
    </div>
    <div class="nutrient-tracker-circle">
      <%= render '/shared/nutrient_tracker/circle_tracker' %>
    </div>
    <div class="nutrient-tracker-recipes">
      <%= render '/shared/nutrient_tracker/suggested_recipes' %>
    </div>
  </div>
</div>

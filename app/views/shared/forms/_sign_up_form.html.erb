<div class="container complete-form">
  <h2>Sign Up</h2>
  <%= simple_form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= f.error_notification %>
    <%= f.input :email,
                label: false,
                placeholder: 'Email',
                required: true,
                autofocus: true ,
                input_html: { autocomplete: "email" }%>
    <%= f.input :password,
                label: false,
                placeholder: 'Password',
                required: true%>
    <%= f.button :submit, "SIGN UP", class: "btn btn-action btn-light-orange" %>
  <% end %>
</div>


